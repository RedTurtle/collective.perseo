<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">

 <object name="collective_perseo_templates"
    meta_type="Filesystem Directory View"
    directory="collective.perseo:skins/collective_perseo_templates"/>
  <object name="collective_perseo_styles"
    meta_type="Filesystem Directory View"
    directory="collective.perseo:skins/collective_perseo_styles"/>
  <object name="collective_perseo_javascript"
    meta_type="Filesystem Directory View"
    directory="collective.perseo:skins/collective_perseo_javascript"/>

 <skin-path name="*">
  <layer name="collective_perseo_templates" insert-after="custom"/>
  <layer name="collective_perseo_styles" insert-after="collective_perseo_templates"/>
  <layer name="collective_perseo_javascript" insert-after="collective_perseo_styles"/>

 </skin-path>

</object>
